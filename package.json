  },
  "dependencies": {
    "angular": "^1.4.1",
    "body-parser": "~1.13.2",
    "chalk": "^1.1.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^1.2.0",
    "express": "~4.13.1",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.3",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "throng": "^1.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.7.13",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "standard": "^3.0.0",
    "watchify": "^3.2.3"
  }
