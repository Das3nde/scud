{
  "name": "scud",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js"
  },
  "standard": {
    "ignore": [
      "public/**",
      "src/vendor/**"
    ]
  },
  "engines": {
    "node": ">=0.12.6"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
